<%- include('header.ejs') %>  
    
<div class="container-fluid contents d-flex">
    <div class="left">
    </div>
    <div class="right">
        <div class="list-box">
            <form class="registerForm" action="/processRegister" method="post">
                <div class="form-group d-flex">
                <label for="formGroupExampleInput">아이디</label>
                <input name="id" type="text" class="form-control" id="formGroupExampleInput" placeholder="Example input">
                </div>
                <div class="form-group d-flex">
                <label for="password">비밀번호</label>
                <input name="password" type="text" class="form-control" id="password" placeholder="Another input">
                </div>
                <div class="form-group d-flex">
                <label for="passwordCheck">비밀번호 확인</label>
                <input name="passwordCheck" type="text" class="form-control something" id="passwordCheck" placeholder="Another input">
                </div>
                <div class="match"></div>
                <div class="form-group d-flex">
                    <label for="formGroupExampleInput4">이메일</label>
                    <input name="email" type="text" class="form-control" id="formGroupExampleInput4" placeholder="Another input">
                </div>
                <div class="form-group d-flex">
                    <label for="formGroupExampleInput5">닉네임</label>
                    <input name="name" type="text" class="form-control" id="formGroupExampleInput5" placeholder="Another input">
                </div>
                <button type="submit" class="registerSubmitBtn">등록</button>
            </form>
            </div>
    </div>
</div>

<div class="footer">
</div>


<script src="/js/main.js"></script>
<script src="/js/register.js"></script>
<script src="/js/jquery.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
</body>
</html>